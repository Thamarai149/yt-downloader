{
  "name": "ytstreamer007-backend",
  "version": "3.0.0",
  "description": "Modern YouTube downloader backend with real-time progress",
  "main": "src/server.js",
  "type": "module",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "dev-clean": "node -e \"require('child_process').exec('taskkill /f /im node.exe', () => {})\" && npm run dev",
    "test": "jest --coverage",
    "lint": "eslint src/**/*.js",
    "format": "prettier --write src/**/*.js"
  },
  "keywords": [
    "youtube",
    "downloader",
    "video",
    "audio"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "node-telegram-bot-api": "^0.66.0",
    "sanitize-filename": "^1.6.3",
    "socket.io": "^4.6.1",
    "uuid": "^9.0.1",
    "yt-dlp-wrap": "^2.1.1"
  },
  "devDependencies": {
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.1"
  }
}
